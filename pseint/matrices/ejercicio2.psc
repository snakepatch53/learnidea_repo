Algoritmo sin_titulo
	Dimension transportes[2,3]
	Para i<-0 Hasta 1 Hacer
		Para j<-0 Hasta 2 Hacer
			Escribir "Ingrese el transporte en la posicion ", i, ",", j, ": "
			Leer transportes[i,j]
		FinPara
	FinPara
	Para i<-0 Hasta 1 Hacer
		Para j<-0 Hasta 2 Hacer
			Escribir "Transporte en la posicion ", i, ",", j, ": ", transportes[i,j]
		FinPara
	FinPara
FinAlgoritmo
